<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <httpCompression>
            <dynamicTypes>
                <add enabled="true" mimeType="application/json" />
            </dynamicTypes>
            <staticTypes>
                <add enabled="true" mimeType="application/json" />
            </staticTypes>
        </httpCompression>
        <staticContent>
            <clientCache cacheControlCustom="public" cacheControlMode="UseMaxAge" cacheControlMaxAge="30.00:00:00" />
        </staticContent>
    </system.webServer>
</configuration>