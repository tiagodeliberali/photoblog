<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <!--<rewrite>
            <rules>
                <rule name="Rewrite Articles">
                    <match url="^article/([0-9]+)/([_0-9a-z-]+)" />
                    <action type="Rewrite" url="article.aspx?id={R:1}&title={R:2}" />
                </rule>
